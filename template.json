{
  "dependencies": {
    "prop-types": "^15.7.2",
    "redux": "^4.0.4",
    "redux-observable": "^1.2.0",
    "rxjs": "^6.5.3",
    "@babel/preset-flow": "^7.7.4",
    "babel-eslint": "^10.0.3",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-react-app": "^5.1.0",
    "eslint-plugin-flowtype": "^4.5.2",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "flow-bin": "^0.114.0",
    "jest-enzyme": "^7.1.2",
    "node-sass": "^4.13.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "source-map-explorer": "^2.1.2",
    "serve": "^11.2.0"
  },
  "scripts": {
    "env": "chmod +x ./env.sh && ./env.sh && cp ./env-config.js ./public/",
    "prestart": "npm run env",
    "prebuild": "npm run env",
    "docker:build": "docker build -t $npm_package_name:$npm_package_version -t $npm_package_name:latest .",
    "docker:run": "docker run --rm -it -p 8080:80 $npm_package_name:latest",
    "test": "react-scripts test --env=jsdom",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "test:ci": "CI=true react-scripts test --env=jsdom",
    "coverage": "npm run test -- --coverage --watchAll=false",
    "coverage:ci": "CI=true npm run test -- --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "server": "npm run build && serve -S build",
    "postinstall": "echo postinstall"
  },
  "jest": {
    "collectCoverageFrom": ["src/**/*.{js,jsx,ts,tsx}", "!<rootDir>/node_modules/"],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "coverageReporters": ["html", "json", "text"],
    "snapshotSerializers": []
  }
}
